{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "format": "prettier --loglevel=error --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:quiet": "eslint . --quiet",
    "check": "tsc --noEmit && vue-tsc --noEmit",
    "ci": "npm run format:check && npm run lint:quiet && npm run check",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/ui": "^2.18.6",
    "husky": "^9.1.6",
    "javascript-time-ago": "^2.5.11",
    "nuxt": "^3.13.0",
    "object-hash": "^3.0.0",
    "pinia": "^2.2.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.7",
    "@pinia/nuxt": "^0.5.4",
    "@types/object-hash": "^3.0.6",
    "@vueuse/nuxt": "^12.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vue-tsc": "^2.1.6"
  }
}
